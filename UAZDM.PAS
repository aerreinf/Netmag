procedure TdmodAz.ibqShowArtPerListiniCalcFields(DataSet: TDataSet);
Var
 dPrezzo,dIVA,dIvato,dRicaricoLire: Currency;
 iSc1,iSc2,iSc3,iRicPerc: Integer;
 iBase: Integer;
 cBaseTipo: String[1];
 strCod_Art_Lst: String;
Begin
 dIvato:=0;
 dPrezzo:=0;
 Try
  If (ibqShowArtPerListiniDINAMICO.AsInteger=0) 
  Then 
   Begin
     dPrezzo:=ibqShowArtPerListiniPREZZO.AsFloat;
     dIvato:=ibqShowArtPerListiniPREZZO_IVATO.AsFloat;
   End
  Else
   Begin
     strCod_Art_Lst:=ibqShowArtPerListiniCODICE_ARTICOLO.AsString;
     
     dRicaricoLire:=ibqShowArtPerListiniRICARICA.AsFloat;
     iRicPerc:=ibqShowArtPerListiniRICARICA_PERC.AsInteger;
     If (ibqShowArtPerListiniSCONTATO.AsInteger=1) 
      Then Begin
            iSc1:=ibqShowArtPerListiniSCONTO1.AsInteger;      
            iSc2:=ibqShowArtPerListiniSCONTO2.AsInteger;      
            iSc3:=ibqShowArtPerListiniSCONTO3.AsInteger;      
           End
      Else Begin
            iSc1:=0;
            iSc2:=0;
            iSc3:=0;      
           End;
     iBase:=ibqShowArtPerListiniBASE.AsInteger;
     cBaseTipo:=ibqShowArtPerListiniBASE_TIPO.AsString;
     dIVA:=ibqShowArtPerListiniIVA.AsFloat;
     If (ibTab_Articoli.Locate('CODICE_ARTICOLO',strCod_Art_Lst,[]))
      Then If (ibqContabile_Articolo.Locate('CODICE_ARTICOLO',strCod_Art_Lst,[]))
            Then Begin
                  Try
                   Case (iBase) Of
                    0: If (cBaseTipo='C')   
                         Then dPrezzo:=ibTab_ArticoliCOSTO_STANDART.asfloat
                         Else dPrezzo:=ibTab_ArticoliPREZZO_BASE.asfloat;
                    1: If (cBaseTipo='A')
                         Then dPrezzo:=ibqContabile_ArticoloULT_COSTO_ACQ.asfloat
                         Else dPrezzo:=ibqContabile_ArticoloULT_PREZZO_VEND.asfloat;
                    2: If (cBaseTipo='A')
                         Then dPrezzo:=ibqContabile_ArticoloMEDIO_COSTO_ACQ.asfloat
                         Else dPrezzo:=ibqContabile_ArticoloMED_PREZZO_VEND.asfloat;
                   End; 

                   dPrezzo:=dPrezzo+dRicaricoLire+dPrezzo*iRicPerc/100;
                   dPrezzo:=dPrezzo+ dPrezzo*isc1/100+
                                     dPrezzo*isc2/100+
                                     dPrezzo*isc3/100;
                   
                   dIvato:= (dIVA*dPrezzo/100+dPrezzo);
                  Except
                    dPrezzo:=ibqShowArtPerListiniPREZZO.AsFloat;
                    dIvato:=ibqShowArtPerListiniPREZZO_IVATO.AsFloat;
                  End;
                 End
            Else dPrezzo:=ibqShowArtPerListiniPREZZO.AsFloat
      Else dPrezzo:=ibqShowArtPerListiniPREZZO.AsFloat;
   End;
 Finally
  ibqShowArtPerListiniPREZZO_.AsFloat:=dPrezzo;
  ibqShowArtPerListiniPREZZO_IVATO_.AsFloat:=dIvato;
 End;
End;

