procedure TdmodAz.ibqryListiniPerArtCalcFields(DataSet: TDataSet);
Var
 dPrezzo,dIVA,dIvato,dRicaricoLire:Currency;
 iSc1,iSc2,iSc3,iRicPerc: Integer;
 iBase: Integer;
 cBaseTipo: String[1];
 strCod_Art_Lst: String;
Begin
//
 dPrezzo:=0;
 dIvato:=0;
 Try
  If (ibqryListiniPerArtDINAMICO.AsInteger=0)
  Then
   Begin
     dPrezzo:=ibqryListiniPerArtPREZZO.AsFloat;
     dIvato:=ibqryListiniPerArtPREZZO_IVATO.AsFloat;
   End
  Else
   Begin
     strCod_Art_Lst:=ibqryListiniPerArtCODICE_ARTICOLO.AsString;
     dRicaricoLire:=ibqryListiniPerArtRICARICA.AsFloat;
     iRicPerc:=ibqryListiniPerArtRICARICA_PERC.AsInteger;
     If (ibqryListiniPerArtSCONTATO.AsInteger=1)
      Then Begin
            iSc1:=ibqryListiniPerArtSCONTO1.AsInteger;
            iSc2:=ibqryListiniPerArtSCONTO2.AsInteger;
            iSc3:=ibqryListiniPerArtSCONTO3.AsInteger;
           End
      Else Begin
            iSc1:=0;
            iSc2:=0;
            iSc3:=0;
           End;
     iBase:=ibqryListiniPerArtBASE.AsInteger;
     cBaseTipo:=ibqryListiniPerArtBASE_TIPO.AsString;
     dIVA:=ibqryListiniPerArtIVA.AsFloat;
                  Try
                   Case (iBase) Of
                    0: If (cBaseTipo='C')
                         Then dPrezzo:=ibqryListiniPerArtCOSTO_STANDART.asfloat
                         Else dPrezzo:=ibqryListiniPerArtPREZZO_BASE.asfloat;
                    1: If (cBaseTipo='A')
                         Then dPrezzo:=ibqryListiniPerArtULT_COSTO_ACQ.asfloat
                         Else dPrezzo:=ibqryListiniPerArtULT_PREZZO_VEND.asfloat;
                    2: If (cBaseTipo='A')
                         Then dPrezzo:=ibqryListiniPerArtMEDIO_COSTO_ACQ.asfloat
                         Else dPrezzo:=ibqryListiniPerArtMED_PREZZO_VEND.asfloat;
                   End;
                   dPrezzo:=dPrezzo+dRicaricoLire+dPrezzo*iRicPerc/100;
                   dPrezzo:=dPrezzo+ dPrezzo*isc1/100+
                                     dPrezzo*isc2/100+
                                     dPrezzo*isc3/100;
                   dIvato:= (dIVA*dPrezzo/100+dPrezzo);
                  Except
                    dPrezzo:=ibqryListiniPerArtPREZZO.AsFloat;
                    dIvato:=ibqryListiniPerArtPREZZO_IVATO.AsFloat;
                  End
   End;
 Finally
  ibqryListiniPerArtPREZZO_.AsFloat:=dPrezzo;
  ibqryListiniPerArtPREZZO_IVATO_.AsFloat:=dIvato;
 End;
End;



